
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>localhost.anyenv-update</string>
    <key>ProgramArguments</key>
    <array>
    	<string>/bin/sh</string>
		<string>-c</string>
        <string>/Users/c-yokoyama/.anyenv/bin/anyenv update &amp;&amp; /usr/local/bin/terminal-notifier -title 'Anyenv Upgrader' -message 'Anyenv upgraded!'</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>StandardErrorPath</key>
	<string>/tmp/localhost.anyenv-upgrade.stderr</string>
	<key>StandardOutPath</key>
	<string>/tmp/localhost.anyenv-upgrade.stdout</string>
    <key>StartCalendarInterval</key>
	<dict>
		<key>Hour</key>
		<integer>12</integer>
	</dict>
</dict>
</plist>

